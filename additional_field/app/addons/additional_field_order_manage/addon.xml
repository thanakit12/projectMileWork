<?xml version="1.0"?>
<addon scheme="2.0">
    <id>additional_field_order_manage</id>
    <name>Additional Field Order Page</name>
    <description>This Addon show field addtional in order manage page(if you want to add field in order manage page you
        can modify in this addon)
    </description>
    <version>1.0.8</version>
    <priority>100502</priority>
    <status>active</status>
    <supplier>Thanakit</supplier>
    <authors>
        <author>
            <name>thanakit</name>
        </author>
    </authors>
    <settings edition_type="ROOT">
        <sections>
            <section id="clean_data">
                <items>
                    <item id="clean_data_info">
                        <type>info</type>
                        <handler>fn_additional_field_order_manage_clean_data</handler>
                    </item>
                </items>
            </section>
            <section id="clean_phone_format_in_order_page">
                <items>
                    <item id="clean_phone_format">
                        <type>info</type>
                        <handler>fn_additional_field_order_manage_clean_phone_format</handler>
                    </item>
                </items>
            </section>
        </sections>
    </settings>
    <compatibility>
        <core_edition>
            MULTIVENDOR
        </core_edition>
    </compatibility>
</addon>